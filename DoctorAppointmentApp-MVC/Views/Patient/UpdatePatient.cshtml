@model DoctorAppointmentApp_MVC.Models.Patient



<form method="post" asp-action="UpdatePatient">
    <div class="w-25">
         <input type="hidden" asp-for=PatientId/>
    <div class="form-group ">
    <input  class="form-control m-2" id="name" asp-for=PatientName placeholder="Enter Patient Name" disabled>
  </div>
  <div class="form-group">
    <input class="form-control m-2" id="email" asp-for=Email placeholder="Enter Patient Email">
  </div>
   <div class="form-group">
    <input class="form-control m-2" id="Contact" asp-for=Contact placeholder="Enter Patient Contact">
  </div>
   <div class="form-group">
    <input  class="form-control m-2" id="history" asp-for=PatientHistory placeholder="Enter Patient History">
  </div>
  <button type="submit" class="btn btn-primary m-2">Submit</button>
    
  </div>

</form>